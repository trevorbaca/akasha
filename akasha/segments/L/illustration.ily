    % [L ViolaMusicVoice measure 338 / measure 5]                              %! _comment_measure_numbers
    R1 * 3/2                                                                   %! _make_measure_silences
    
    % [L ViolaMusicVoice measure 339 / measure 6]                              %! _comment_measure_numbers
    R1 * 3/4                                                                   %! _make_measure_silences
    
}


